@model Dictionary<long, string>

<h2>圖片列表</h2>

@if (Model != null && Model.Count > 0)
{
    foreach (var item in Model)
    {
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3  item">
            <div>
                <img src="data:image/png;base64,@Html.Raw(item.Value)" />
            </div>
        </div>
    }
